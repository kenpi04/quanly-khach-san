@model List<QLKhachSanWeb.Models.UserModel>

<h1>Danh sách nhân viên</h1>
           
             @using (Ajax.BeginForm(new AjaxOptions {HttpMethod="POST" }))
             {
                 <div class="pcontent">
                <table class="form1">
                    <tr>
                        <td class="lb">Họ và tên</td>
                        <td class="lb">Tên đăng nhập</td>
                        <td class="lb">Mật Khẩu</td>
                        <td class="lb">  </td>
                    </tr>
                    @foreach(QLKhachSanWeb.Models.UserModel team in Model)
                    {
                    <tr>
                        <td class="lb">@team.Name</td>
                        <td class="lb">@team.UserName</td>
                        <td class="lb">@team.Password</td>
                        <td class="lb">
                           @Html.ActionLink("Cập nhật","UpdateUser","Admin",new { team.Id },null)
                        </td>
                    </tr>
                    }
                </table>
                <div class="submit-form">
                    <input type="button" id="btnCancel" value="Thoát"/>
                </div>
            </div>
             }