@{
    ViewBag.Title = "XemLog";
    Layout = "~/Views/Shared/_AdminRoot.cshtml";
}
@model QLKhachSanWeb.Models.ListLogModel
<table>
    <tr>
        <td>Mã nhật kí</td>
        <td>Ngày thực hiện</td>
        <td>Hành động</td>
        <td>Người thực hiện</td>
    </tr>
     @foreach (QLKhachSanWeb.Models.LogModel team in Model.ListLogs)
     {
         <tr>
             <td>@team.id</td>
             <td>@team.CreateDate</td>
             <td>@team.ActionName</td>
             <td>@team.UserName</td>
         </tr>
     }
    <tr>
        <td>
            <div>Trang:
            @{for (int i = 1; i <= Model.PageInfo.TotalPages; i++)
              {
                <a href="@Url.Action("XemLog","Admin", new { page=@i})">@i</a>
              }
            }
            </div>
        </td>
    </tr>
</table>
 